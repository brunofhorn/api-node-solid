    "@vitest/coverage-v8": "3.1.2",
    "@vitest/ui": "3.1.2",
    "eslint": "8.57.1",
    "prisma": "6.6.0",
    "supertest": "7.1.0",
    "tsup": "^8.4.0",
    "tsx": "^4.19.3",
    "typescript": "^5.8.3",
    "vite-tsconfig-paths": "5.1.4",
    "vitest": "3.1.2"
  },
  "dependencies": {
    "@fastify/jwt": "9.1.0",
